{
  "name": "eloquent-js",
  "version": "0.0.1",
  "description": "Node.js project template for Eloquent JavaScript examples (browser)",
  "main": "./bin/www",
  "engines": {
    "node": "13.x.x",
    "npm": "6.13.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sergiofgonzalez/eloquent-js.git"
  },
  "keywords": [],
  "author": {
    "name": "Sergio F. Gonzalez",
    "email": "sergio.f.gonzalez@gmail.com"
  },
  "config": {
    "debugModules": "lib:*"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "6.8.0"
  },
  "dependencies": {
    "compression": "1.7.4",
    "debug": "4.1.1",
    "ejs": "3.0.1",
    "express": "4.17.1",
    "helmet": "3.21.2",
    "js-yaml": "3.13.1",
    "morgan": "1.9.1",
    "nconf": "0.10.0",
    "serve-favicon": "2.5.0",
    "winston": "3.2.1"
  },
  "scripts": {
    "prestart": "npm run lint",
    "start": "DEBUG=${DEBUG:-$npm_package_config_debugModules} node app/src/${npm_package_main}",
    "lint": "node_modules/.bin/eslint app/src/",
    "predebug": "npm run lint",
    "debug": "DEBUG=${DEBUG:-$npm_package_config_debugModules} node --inspect-brk=${npm_config_debugHost:-127.0.0.1} app/src/${npm_package_main}",
    "freshen-deps": "./scripts/freshen.sh"
  }
}
